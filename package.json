  "devDependencies": {
    "chai": "^3.5.0",
    "jshint": "^2.9.2",
    "mocha": "^2.4.5",
    "np": "pimterry/np",
    "watch": "^0.18.0"
  },
  "dependencies": {
    "callsite": "^1.0.0",
